alias bringup='ros2 launch turtlebot3_bringup robot.launch.py'
alias teleop='ros2 run turtlebot3_teleop teleop_keyboard'
alias slam='ros2 launch slam_toolbox online_async_launch.py use_sim_time:=false params_file:=$HOME/slam_params.>'
alias savemap='ros2 run nav2_map_server map_saver_cli -f'
alias joystick='ros2 run joy joy_node'
alias battery='ros2 topic echo /battery_state --field percentage'